        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/webapp",
            "env": {},
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        
    ]
